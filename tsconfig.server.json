{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022"],
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/server",
    "rootDir": "./server",
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["server/**/*"],
  "exclude": ["node_modules"]
}
